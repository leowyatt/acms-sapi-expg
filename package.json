{
  "name": "acms-sapi-expg",
  "version": "1.0.0",
  "author": "leowyatt",
  "license": "mit",
  "private": true,
  "scripts": {
    "start": "node src/index.js",
    "pull": "git pull",
    "build": "yarn dcd build --no-cache",
    "up": "yarn dcd up -d --build",
    "restart": "yarn dcd restart",
    "down": "yarn dcd down",
    "logs": "yarn dcd logs -f",
    "patch": "yarn version --patch",
    "rm": "docker volume prune",
    "rms": "docker system prune -a --volumes",
    "update": "yarn pull; yarn install; yarn up; sleep 10; yarn migrate up; sleep 10; yarn restart",
    "migrate": "yarn db-migrate",
    "save": "mkdir -p tmp; cp tmp/backup.tar.gz tmp/$(date '+%Y-%m-%d_%H:%M').tar.gz; docker save $(docker image ls -q) | gzip > tmp/backup.tar.gz",
    "load": "docker load < tmp/backup.tar.gz",
    "backup": "",
    "restore": "",
    "dcd": "docker compose --profile dev"
  },
  "comments": {
    "scripts": {
      "pull": "git pull",
      "build": "build no-cache",
      "up": "start with build",
      "restart": "restart",
      "down": "stop",
      "logs": "logs",
      "patch": "version patch",
      "rm": "volumes prune",
      "rms": "system prune all",
      "update": "app pull and install, sapi update",
      "migrate": "migrate database"
    }
  },
  "dependencies": {
    "@fullstackio/postgraphile-upsert-plugin": "1.4.0",
    "@graphile-contrib/pg-many-to-many": "1.0.2",
    "@graphile-contrib/pg-simplify-inflector": "6.1.0",
    "@graphile/operation-hooks": "1.0.0",
    "@graphile/pg-aggregates": "0.1.0",
    "@graphile/subscriptions-lds": "4.13.0",
    "apollo-server-core": "3.10.3",
    "apollo-server-express": "3.10.3",
    "aws-sdk": "^2.1304.0",
    "body-parser": "1.20.1",
    "boolean": "3.2.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "db-migrate": "0.11.13",
    "db-migrate-pg": "1.2.2",
    "dotenv": "16.0.3",
    "email-templates": "10.0.1",
    "expo-server-sdk": "^3.7.0",
    "express": "4.18.2",
    "fs-extra": "^11.1.0",
    "graphile-utils": "4.13.0",
    "graphql": "16.6.0",
